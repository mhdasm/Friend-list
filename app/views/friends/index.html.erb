<h1>Friend List</h1>

<div id="friends">
<table class="table table-hover table-bordered">
 <thead class="table-dark" >
  <tr>
   <th>First name</th>
   <th>Last name</th>
   <th>Email</th>
   <th>Phone</th>
   <th>Twitter</th>
   <th colspan="3"></th>
  </tr>
 </thead>
 <tbody>
 <% @friends.each do |friend| %>
  <%= render friend %>
<% end %>       
  </tbody>
 </table>
  
</div>

<%= link_to "New friend", new_friend_path , class:"btn btn-dark"%>
